{
  "name": "stampless_frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{css,vue} --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "lint:fix": "yarn lint:js --fix && yarn lint:style --fix",
    "test": "TZ=UTC jest --passWithNoTests --maxWorkers=50%",
    "test:coveralls": "TZ=UTC jest --passWithNoTests --ci && coveralls < coverage/lcov.info",
    "test:sonar": "TZ=UTC jest --passWithNoTests --ci --reporters=default --reporters=jest-sonar",
    "heroku-postbuild": "yarn generate",
    "prepare": "node scripts/prepare.js"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ],
    "*.{css,vue}": "stylelint --fix"
  },
